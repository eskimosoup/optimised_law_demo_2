<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= case_study_category_presenter.id %>">



    <div class="large-8 small-8 columns" data-equalizer-watch="module-list">

    <%= case_study_category_presenter.name %>
  </div>



  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= case_study_category_presenter.toggle_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= case_study_category_presenter.view_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= case_study_category_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%= case_study_category_presenter.delete_link %>
  </div>
</div>
