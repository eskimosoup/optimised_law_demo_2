<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= tour_entry_presenter.id %>">

    <div class="large-1 hide-for-medium-down columns" data-equalizer-watch="module-list">
      <span class="drag-helper"></span>
    </div>



    <div class="large-3 small-8 columns" data-equalizer-watch="module-list">

    <%= tour_entry_presenter.toggle_title %>
  </div>

    <div class="large-2 small-8 columns" data-equalizer-watch="module-list">

    <%= tour_entry_presenter.page_title %>
  </div>

    <div class="large-2 small-8 columns" data-equalizer-watch="module-list">

    <%= tour_entry_presenter.page_area %>
  </div>



  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= tour_entry_presenter.toggle_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= tour_entry_presenter.view_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= tour_entry_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%= tour_entry_presenter.delete_link %>
  </div>

  <div class="row module-list-detail hide" id="index-list-<%= tour_entry_presenter.id %>">
    <div class="small-12 columns module-list-summary last">
      <%= tour_entry_presenter.optimadmin_summary %>
    </div>
  </div>
</div>
